<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.atguigu.environment.mapper.DepartmentMapper">
	
	<select id="getDepartmentListByCityId" parameterType="Integer" resultType="com.atguigu.environment.model.Department">
		select id,name from department where cityId=#{cityId}
	</select>
	
</mapper>


















